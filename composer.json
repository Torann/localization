{
  "name": "torann/localization",
  "description": "Simple subdomain localization for Laravel.",
  "keywords": [
    "localization",
    "laravel",
    "locale",
    "subdomain"
  ],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Daniel Stainback",
      "email": "torann@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "illuminate/routing": "^8.0|^9.0|^10.0",
    "illuminate/http": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.3",
    "phpstan/phpstan": "^0.12.14",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Torann\\Localization\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
